{
  "name": "@chendf/ep-toolkit",
  "version": "0.0.1",
  "type": "module",
  "module": "dist/es/index.js",
  "types": "dist/es/index.d.ts",
  "style": "dist/index.css",
  "files": [
    "dist",
    "package.json",
    "README.md",
    "LICENSE",
    "web-types.json"
  ],
  "exports": {
    ".": {
      "import": "./dist/es/index.js",
      "types": "./dist/es/index.d.ts"
    },
